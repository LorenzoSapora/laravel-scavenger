{
  "name": "reliqarts/scavenger",
  "description": "Highly configurable Laravel 5 scraper package.",
  "keywords": [
    "scraper",
    "package",
    "generation",
    "laravel",
    "scavenger",
    "laravel5",
    "seek",
    "content",
    "data",
    "web"
  ],
  "version": "2.3.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "illuminate/support": "^5.5",
    "monolog/monolog": "~1.11",
    "fabpot/goutte": "^3.2",
    "ext-iconv": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\Scavenger\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["ReliqArts\\Scavenger\\ServiceProvider"],
      "aliases": {
        "ScavengerSeeker": "ReliqArts\\Scavenger\\Services\\Seeker"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
