{
  "name": "reliqarts/laravel-scavenger",
  "description": "Highly configurable Laravel 6.x scraper package.",
  "keywords": [
    "scraper",
    "package",
    "generation",
    "laravel",
    "scavenger",
    "laravel5",
    "seek",
    "content",
    "data",
    "web"
  ],
  "type": "library",
  "license": "MIT",
  "version": "3.0.0",
  "authors": [
    {
      "name": "reliq",
      "email": "reliq@reliqarts.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/support": "^6.0",
    "monolog/monolog": "~1.11",
    "fabpot/goutte": "^3.2",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "orchestra/testbench": "^4.0",
    "phpro/grumphp": "^0.15.2",
    "wearejust/grumphp-extra-tasks": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "ReliqArts\\Scavenger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReliqArts\\Scavenger\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": ["ReliqArts\\Scavenger\\ServiceProvider"],
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
